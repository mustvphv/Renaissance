<?xml version="1.0" encoding="UTF-8"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

    <servlet>
	    <servlet-name>ServletRen</servlet-name>
	    <servlet-class>com.ren.servlets.ServletRen</servlet-class>
	    
	    	    	
	    <init-param>
		    <param-name>auteur</param-name> 
		    <param-value>mustvphv</param-value> 
	    </init-param>

	    <load-on-startup>1</load-on-startup>
	    
    </servlet>
    
    <servlet-mapping>
		<servlet-name>ServletRen</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>UserAccess</servlet-name>
		<servlet-class>com.user_access.servlets.UserAccess</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UserAccess</servlet-name>
		<url-pattern>/user_access</url-pattern>
	</servlet-mapping>
    
    

</web-app>